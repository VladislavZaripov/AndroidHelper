<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:padding="5dp"
        tools:context=".Activity_34">
    <LinearLayout
            android:id="@+id/linearLayout1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentTop="true"
            android:layout_marginTop="10dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentEnd="true">
        <Button
                android:id="@+id/btnStartHttp"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:onClick="onClickStart"
                android:text="@string/Act34_http"
                android:textSize="12sp" tools:ignore="ButtonStyle">
        </Button>
        <Button
                android:id="@+id/btnStartStream"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:onClick="onClickStart"
                android:text="@string/Act34_stream"
                android:textSize="12sp" tools:ignore="ButtonStyle">
        </Button>
        <Button
                android:id="@+id/btnStartSD"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:onClick="onClickStart"
                android:text="@string/Act34_sd"
                android:textSize="12sp" tools:ignore="ButtonStyle">
        </Button>
        <Button
                android:id="@+id/btnStartUri"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:onClick="onClickStart"
                android:text="@string/Act34_uri"
                android:textSize="12sp" tools:ignore="ButtonStyle">
        </Button>
        <Button
                android:id="@+id/btnStartRaw"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:onClick="onClickStart"
                android:text="@string/Act34_raw"
                android:textSize="12sp" tools:ignore="ButtonStyle">
        </Button>
    </LinearLayout>
    <LinearLayout
            android:id="@+id/linearLayout2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentRight="true"
            android:layout_below="@+id/linearLayout1"
            android:layout_marginTop="30dp" android:layout_alignParentStart="true" android:layout_alignParentEnd="true">
        <Button
                android:id="@+id/btnPause"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:onClick="onClick"
                android:text="@string/Act34_pause"
                android:textSize="12sp">
        </Button>
        <Button
                android:id="@+id/btnResume"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:onClick="onClick"
                android:text="@string/Act34_resume"
                android:textSize="12sp">
        </Button>
        <Button
                android:id="@+id/btnStop"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:onClick="onClick"
                android:text="@string/Act34_stop"
                android:textSize="12sp">
        </Button>
        <CheckBox
                android:id="@+id/chbLoop"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/Act34_loop">
        </CheckBox>
    </LinearLayout>
    <LinearLayout
            android:id="@+id/linearLayout3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentRight="true"
            android:layout_below="@+id/linearLayout2"
            android:layout_marginTop="10dp" android:layout_alignParentStart="true" android:layout_alignParentEnd="true">
        <Button
                android:id="@+id/btnBackward"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:onClick="onClick"
                android:text="@string/Act34_backward"
                android:textSize="12sp" tools:ignore="ButtonStyle">
        </Button>
        <Button
                android:id="@+id/btnForward"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:onClick="onClick"
                android:text="@string/Act34_forward"
                android:textSize="12sp" tools:ignore="ButtonStyle">
        </Button>
        <Button
                android:id="@+id/btnInfo"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:onClick="onClick"
                android:text="@string/Act34_info"
                android:textSize="12sp" tools:ignore="ButtonStyle">
        </Button>
    </LinearLayout>
</RelativeLayout>