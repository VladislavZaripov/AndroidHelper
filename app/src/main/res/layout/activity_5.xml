<?xml version="1.0" encoding="utf-8"?>
    <TableLayout
            xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:tools="http://schemas.android.com/tools"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            tools:context=".Activity_5"
            android:orientation="vertical"
            android:background="#76E0ED">

    <TableRow android:layout_width="match_parent"
              android:layout_height="wrap_content"
              android:layout_marginTop="200dp">
        <EditText
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:inputType="textPersonName"
                android:id="@+id/Act5_editText"
                android:gravity="center"
                android:layout_weight="3"
                android:background="#CDDC39"
                android:textSize="24sp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:autofillHints="no"
                tools:ignore="LabelFor"/>
        <Button
                android:text="@string/Act5_button1"
                android:layout_width="150dp"
                android:layout_height="wrap_content"
                android:id="@+id/Act5_button1"
                android:layout_marginRight="10dp"
                android:layout_marginEnd="10dp"/>
    </TableRow>
    <TableRow android:layout_width="match_parent"
              android:layout_height="match_parent"
              android:layout_marginTop="100dp">
        <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/Act5_textView"
                android:layout_weight="3"
                android:gravity="center"
                android:background="#CDDC39"
                android:textSize="24sp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"/>
        <Button
                android:text="@string/Act5_button2"
                android:layout_width="150dp"
                android:layout_height="wrap_content"
                android:id="@+id/Act5_button2"
                android:layout_marginRight="10dp"
                android:layout_marginEnd="10dp"/>
    </TableRow>
    </TableLayout>